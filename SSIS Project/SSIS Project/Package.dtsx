                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
  s.SalesOrderID,
  o.SalesOrderDetailID,
  p.ProductID,
  ISNULL(c.CustomerID, 0) AS [CustomerID],
  c.PersonID,
  ISNULL(c.StoreID, 0) AS [StoreID],
  CASE
    WHEN c.PersonID IS NOT NULL AND c.StoreID IS NULL THEN 'B2C'
    WHEN c.PersonID IS NOT NULL AND c.StoreID IS NOT NULL THEN 'B2B'
  END AS CustomerType,
  p.ProductCategoryID,
  p.ProductSubcategoryID,
  ISNULL(l.AddressID, 0) AS [AddressID],
  ISNULL(mkt.ReasonName, 'No Reason') AS [ReasonName],
  ISNULL(mkt.ReasonType, 'No Reason type') AS [ReasonType],
  s.TotalQuantity,
  s.TotalDue,
  o.LineTotal,
  o.TotalStandardCost,
  o.LineTotal - o.TotalStandardCost AS OrderDetailProfit,
  t.FullDateAlternateKey AS DateKey
FROM [dbo].[Dim_SalesOrder] s
LEFT JOIN [dbo].[Dim_OrderDetail] o ON s.SalesOrderID = o.SalesOrderID
LEFT JOIN [dbo].[Dim_Customer] c ON s.CustomerID = c.CustomerID
LEFT JOIN [dbo].[Dim_Product] p ON o.ProductID = p.ProductID
LEFT JOIN [dbo].[Dim_Location] l ON s.CustomerID = l.CustomerID
LEFT JOIN [dbo].[Sub-Dim_Reason] sd ON s.SalesOrderID = sd.SalesOrderID
LEFT JOIN [dbo].[Dim_MKT] mkt ON sd.SalesReasonID = mkt.SalesReasonID
LEFT JOIN [dbo].[Dim_Time] t ON s.OrderDate = t.FullDateAlternateKey;</property>
